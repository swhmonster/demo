<?xml version="1.0" encoding="GB2312"?>
<xsd:schema  xmlns="http://dataexchange.court.gov.cn/2009/data"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://dataexchange.court.gov.cn/2009/data"
elementFormDefault="qualified">

  <xsd:include schemaLocation="_A_类型_基础.xsd"/>

  <xsd:complexType name="收案和立案_纪">
    <xsd:sequence>
      <xsd:element name="案号"         type="C50" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查起始日期" type="D"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="被审查人_纪">
    <xsd:sequence>
      <xsd:element name="姓名"                 type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="性别"                 type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="出生日期"             type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="年龄"                 type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="民族"                 type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="文化程度"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="政治面貌"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行政级别"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="工作部门标识"         type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="职务"                 type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法院工作时间"         type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="参加工作时间"         type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="主要违法违纪类型"     type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="发生时间"             type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="发现时间"             type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="线索来源"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="初查机关类型"         type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="初查机关名称"         type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="采取措施"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="利用何种权利违法违纪" type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="备注"                 type="BB"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="违法违纪情况_纪1">
    <xsd:sequence>
      <xsd:element name="序号_违法违纪"        type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="违法违纪类型"         type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="发生时间"             type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="发现时间"             type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="线索来源"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="初查机关类型"         type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="初查机关名称"         type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="采取措施"             type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="利用何种权利违法违纪" type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="备注"                 type="BB"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="违法违纪情况_纪">
    <xsd:sequence>
      <xsd:element name="R" type="违法违纪情况_纪1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="案件审理情况_纪">
    <xsd:sequence>
      <xsd:element name="审查处理时间" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="处理类型"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="处理结果"     type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
