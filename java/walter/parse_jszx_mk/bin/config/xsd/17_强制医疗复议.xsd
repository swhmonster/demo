<?xml version="1.0" encoding="GB2312"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2009/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2009/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0_结构_复用.xsd"/>
  <CASE:include schemaLocation="_1_结构_刑事.xsd"/>

  <CASE:complexType name="强制医疗复议">
    <CASE:sequence>
      <CASE:element name="案件标识"           type="案件标识"          minOccurs="1" maxOccurs="1"/>
      <CASE:element name="原审情况"           type="原审情况_强医"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="后续情况"           type="后续情况_强医"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="收案和立案信息"     type="收案和立案_强医"   minOccurs="0" maxOccurs="1"/>
      <CASE:element name="办案期限"           type="办案期限"          minOccurs="0" maxOccurs="1"/>
      <CASE:element name="案件审理情况"       type="案件审理_强医"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="结案情况"           type="结案情况_强医"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="被申请人"           type="被申请人_强医"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="申请人"             type="申请人_强医"       minOccurs="0" maxOccurs="1"/>
      <CASE:element name="审判组织成员"       type="审判组织成员"      minOccurs="0" maxOccurs="1"/>
      <CASE:element name="法庭使用记录"       type="法庭使用记录_强医" minOccurs="0" maxOccurs="1"/>
      <CASE:element name="文书"               type="文书"              minOccurs="0" maxOccurs="1"/>
      <CASE:element name="审理阶段"           type="审理阶段"          minOccurs="0" maxOccurs="1"/>
      <CASE:element name="与案实体名录"       type="与案实体名录"      minOccurs="0" maxOccurs="1"/>
    </CASE:sequence>
  </CASE:complexType>
  
  <CASE:element name="强制医疗复议案件" type="强制医疗复议"/>

</CASE:schema>
