<?xml version="1.0" encoding="GB2312"?>
<xsd:schema  xmlns="http://dataexchange.court.gov.cn/2009/data"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://dataexchange.court.gov.cn/2009/data"
elementFormDefault="qualified">

  <xsd:include schemaLocation="_A_类型_基础.xsd"/>
  <xsd:include schemaLocation="_0_结构_复用.xsd"/>

  <xsd:complexType name="案件审理_赔">
    <xsd:sequence>
      <xsd:element name="接收案件日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="重新审理类型"     type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办审判庭标识"   type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办审判庭"       type="C40" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办人"           type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="庭前证据交换"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法院收集调取证据" type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="地域涉及"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件大小影响"     type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理报告"         type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="公开审理"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="不公开审理原因"   type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="委托事项"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="受托法院"         type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="受指定或移送法院" type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="开庭审理"         type="TN"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="当事人_赔1">
    <xsd:sequence>
      <xsd:element name="序号_当事人" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="当事人"      type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行为能力"    type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="起诉对象"    type="BB" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="当事人_赔">
    <xsd:sequence>
      <xsd:element name="R" type="当事人_赔1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="赔偿义务机关1">
    <xsd:sequence>
      <xsd:element name="序号_赔偿义务机关" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿义务机关类型"  type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿义务机关"      type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿决定日期"      type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿决定金额"      type="M"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿决定文书"      type="SN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="赔偿义务机关">
    <xsd:sequence>
      <xsd:element name="R" type="赔偿义务机关1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="复议机关1">
    <xsd:sequence>
      <xsd:element name="序号_复议机关" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="复议机关"      type="SN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="复议机关">
    <xsd:sequence>
      <xsd:element name="R" type="复议机关1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="不予受理复议">
    <xsd:sequence>
      <xsd:element name="申请复议日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="复议处理日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="复议处理结果" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="移送案件日期" type="D"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="不予确认申诉">
    <xsd:sequence>
      <xsd:element name="申诉日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申诉处理日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申诉处理结果" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="移送案件日期" type="D"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:group name="结案情况">
    <xsd:sequence>
      <xsd:element name="结案日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="结案案由"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="旧结案案由"   type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="结案方式"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="撤销变更原因" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿方式"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="精神损害抚慰金"  type="M" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="协商赔偿"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="不予受理原因" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="生效日期"     type="D"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:group>
  <xsd:complexType name="结案情况_赔确">
    <xsd:sequence>
      <xsd:group ref="结案情况" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="结案情况_法赔">
    <xsd:sequence>
      <xsd:group    ref="结案情况"          minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿金额" type="M" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="新结案方式" type="N" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="是否有结案文书"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="无结案文书理由"   type="C240"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="收案和立案_赔确">
    <xsd:sequence>
      <xsd:element name="案号"         type="C50" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案途径"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件来源"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收到申请日期" type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请书"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="随案卷宗材料" type="BB"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案案由"     type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="旧立案案由"   type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="重新申请"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门标识" type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门"     type="C40" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案登记人"   type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查人"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查意见"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批人"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案审批日期" type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批意见"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审判流程已公开"   type="N"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="收案和立案_法赔">
    <xsd:sequence>
      <xsd:element name="案号"         type="C50" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案途径"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件来源"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收到材料日期" type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请书"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="随案卷宗材料" type="BB"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案案由"     type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="旧立案案由"   type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请事由"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="请求赔偿类型" type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="请求赔偿金额" type="M"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门标识" type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门"     type="C40" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案登记人"   type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查人"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查意见"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批人"       type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案审批日期" type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批意见"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件子类型"   type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="新案件来源"   type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审判流程已公开"   type="N"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="办案期限_赔确">
    <xsd:sequence>
      <xsd:element name="上级限令裁定期限" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限起始日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限届满日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案期限届满日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理结束日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定立案期限"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际立案天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案超期天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定审限天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际审理天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限天数"       type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限倍数"       type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限原因"       type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="办案期限_法赔">
    <xsd:sequence>
      <xsd:element name="上级限令裁定期限" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限起始日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限届满日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案期限届满日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理结束日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定立案期限"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际立案天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案超期天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定审限天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际审理天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限天数"       type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限倍数"       type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限原因"       type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
