<?xml version="1.0" encoding="GB2312"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2009/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2009/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0_结构_复用.xsd"/>
  <CASE:include schemaLocation="_9_结构_纪检监察.xsd"/>

  <CASE:complexType name="纪检监察">
    <CASE:sequence>
      <CASE:element name="案件标识"       type="案件标识"        minOccurs="1" maxOccurs="1"/>
      <CASE:element name="收案和立案信息" type="收案和立案_纪"   minOccurs="0" maxOccurs="1"/>
      <CASE:element name="被审查人"       type="被审查人_纪"     minOccurs="0" maxOccurs="1"/>
      <CASE:element name="违法违纪情况"   type="违法违纪情况_纪" minOccurs="0" maxOccurs="1"/>
      <CASE:element name="案件审理情况"   type="案件审理情况_纪" minOccurs="0" maxOccurs="1"/>
    </CASE:sequence>
  </CASE:complexType>

  <CASE:element name="纪检监察案件" type="纪检监察"/>

</CASE:schema>
