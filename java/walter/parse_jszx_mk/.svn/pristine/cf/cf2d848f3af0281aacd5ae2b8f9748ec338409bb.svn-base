<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0A_类型_基础.xsd" />
  <CASE:complexType name="回避情况1">
    <CASE:sequence>
		<CASE:element name="案件标识"		type="ID"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="序号_回避"		type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="回避类型"		type="SDM"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="申请回避日期"	type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="回避原因"		type="SDM"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="回避人"			type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="回避申请人"		type="SN"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="回避处理结果"	type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="回避决定人"		type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="回避决定日期"	type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="回避决定书"		type="SN"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="更新时间" 		type="DT" 	minOccurs="1" maxOccurs="1"/>   
    </CASE:sequence>
  </CASE:complexType>
  <CASE:complexType name="回避情况">
    <CASE:sequence>
		<CASE:element name="回避情况" type="回避情况1"  minOccurs="0" maxOccurs="32768"/>
	</CASE:sequence>
  </CASE:complexType>
  <CASE:element name="实施类案件回避情况" type="回避情况"/>
</CASE:schema>