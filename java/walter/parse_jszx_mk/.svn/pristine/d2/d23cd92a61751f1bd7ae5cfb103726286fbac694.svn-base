<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0A_类型_基础.xsd" />
  <CASE:complexType name="送达记录1">
    <CASE:sequence>
		<CASE:element name="案件标识"			type="ID"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="序号_送达"			type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="文书"				type="BB"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="应送达日期"		type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="开始送达日期"		type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="送达方式"			type="SDM"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="受托法院"			type="SN"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="公告日期"			type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="交邮日期"			type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="送达日期"			type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="送达人"				type="N"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="受送达人"			type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="签收人"				type="C40"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="代签人或转交人"	type="C40"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="签收时间"			type="DT"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="拒不签收"			type="B"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="未送达原因"			type="C200"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="更新时间" 			type="DT" 	minOccurs="1" maxOccurs="1"/>   
    </CASE:sequence>
  </CASE:complexType>
  <CASE:complexType name="送达记录">
    <CASE:sequence>
		<CASE:element name="送达记录" type="送达记录1"  minOccurs="0" maxOccurs="32768"/>
	</CASE:sequence>
  </CASE:complexType>
  <CASE:element name="审查类案件送达记录" type="送达记录"/>
</CASE:schema>
