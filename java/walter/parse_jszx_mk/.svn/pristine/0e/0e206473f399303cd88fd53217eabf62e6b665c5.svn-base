<?xml version="1.0" encoding="GB2312"?>
<xsd:schema  xmlns="http://dataexchange.court.gov.cn/2009/data"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://dataexchange.court.gov.cn/2009/data"
elementFormDefault="qualified">

  <xsd:include schemaLocation="_A_类型_基础.xsd"/>
  <xsd:include schemaLocation="_0_结构_复用.xsd"/>

  <xsd:complexType name="当事人1">
    <xsd:sequence>
      <xsd:element name="序号_当事人"  type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="一审诉讼地位" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="二审诉讼地位" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="再审诉讼地位" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="当事人"       type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行为能力"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="附带民事诉讼赔偿信息" type="附带民诉赔偿" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="当事人">
    <xsd:sequence>
      <xsd:element name="R" type="当事人1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="附带民诉赔偿">
    <xsd:sequence>
      <xsd:element name="被请求赔偿金额"   type="M"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="判处赔偿金额"     type="M"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿方式"         type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿给付完毕"     type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿即时给付金额" type="M"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="赔偿即时给付日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="后续审判处理结果" type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="当事人诉讼活动1">
    <xsd:sequence>
      <xsd:element name="序号_诉讼活动"          type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="提诉人"                 type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="被提诉人"               type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="提诉日期"               type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="代为提诉"               type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="代为提诉人与被害人关系" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理要求"               type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请撤诉"               type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="终止诉讼"               type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="应诉人"                 type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="答辩日期"               type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承认提诉事实"           type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请反诉撤诉"           type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="反诉针对对象"           type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="反诉处理结果"           type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="未出庭次数"             type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="未出庭原因"             type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="拒不到庭次数"           type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="无正当理由退庭次数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="拒不到庭或中途退庭处理" type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="辩护类型"               type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="拒绝辩护或代理"         type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="撤诉针对对象"           type="BB" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="接受法律援助原因"       type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="当事人诉讼活动">
    <xsd:sequence>
      <xsd:element name="R" type="当事人诉讼活动1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="收案和立案_监">
    <xsd:sequence>
      <xsd:element name="再审审查案件子类别" type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案号"         type="C50"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案途径"     type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="抗诉机关"     type="C100" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审查监督类型" type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件来源"     type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收到材料日期" type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请书"       type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="随案卷宗材料" type="BB"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请标的物"   type="C200" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="申请标的金额" type="M"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门标识" type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案部门"     type="C40"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="收案登记人"   type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批人"       type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案审批日期" type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审批意见"     type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案日期"     type="D"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="新案件来源"   type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行政管理范围"   type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="依职权启动审查动因"   type="N"    minOccurs="0" maxOccurs="1"/>
      <xsd:element name="再审审查案件来源_2015"   type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件来源_2015"  type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="原生效裁判案由" type="SN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审判流程已公开"   type="N"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="办案期限_监">
    <xsd:sequence>
      <xsd:element name="限期或暂缓期限"   type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限起始日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审限届满日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案期限届满日期" type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理结束日期"     type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定立案期限"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际立案天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="立案超期天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="法定审限天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="实际审理天数"     type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限天数"       type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限倍数"       type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="超审限原因"       type="TN" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="案件审理_监">
    <xsd:sequence>
      <xsd:element name="接收案件日期"     type="D"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办审判庭标识"   type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办审判庭"       type="C40" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="承办人"           type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="地域涉及"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件涉及"         type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="案件大小影响"     type="N"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="审理报告"         type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="公开审理"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="不公开审理原因"   type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="委托事项"         type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="受托法院"         type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="受指定或移送法院" type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="开庭审理"         type="TN"  minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="结案情况_监">
    <xsd:sequence>
      <xsd:element name="结案标的金额"             type="M"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="结案日期"                 type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="结案方式"                 type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="决定再审理由"             type="N" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="指令重新审理或执行的法院" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="生效日期"                 type="D"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="下发法院"                 type="SN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="新结案方式"               type="N"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="是否有结案文书"     type="TN"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="无结案文书理由"     type="C240"  minOccurs="0" maxOccurs="1"/>
      <xsd:element name="结案方式_2015"      type="TN"   minOccurs="0" maxOccurs="1"/>
      <xsd:element name="原生效裁判案由"     type="SN"   minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="附带民诉当事人1">
    <xsd:sequence>
      <xsd:element name="序号_附带民诉当事人" type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="一审诉讼地位"        type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="二审诉讼地位"        type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="再审诉讼地位"        type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="附带民诉当事人"      type="SN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="被告人类型"          type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="行为能力"            type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="系被害人"            type="TN" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="诉讼对象"            type="BB" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="附带民诉当事人">
    <xsd:sequence>
      <xsd:element name="R" type="附带民诉当事人1" minOccurs="0" maxOccurs="32768"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
