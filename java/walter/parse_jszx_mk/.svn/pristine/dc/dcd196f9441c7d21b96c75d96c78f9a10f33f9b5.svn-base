package com.thunisoft.parse09xml.bean;

/**
 * 案件xml信息
 * @author chenss
 *
 */
public class EntityAjXml {
    // 标识
    private String path;
    // 文件内容
    private byte[] data;
    // 文件名称
    private String xmlName;
    
    public EntityAjXml(String path, byte[] data, String xmlName) {
        super();
        this.path = path;
        this.data = data;
        this.xmlName = xmlName;
    }
    public String getPath() {
        return path;
    }
    public byte[] getData() {
        return data;
    }
    public String getXmlName() {
        return xmlName;
    }
}
