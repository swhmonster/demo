<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0A_类型_基础.xsd" />
  <CASE:complexType name="执行担保情况1">
    <CASE:sequence>
		<CASE:element name="案件标识"		type="ID"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="序号_担保"		type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="事由"			type="SDM"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="当事人"			type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="担保方式"		type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保人"			type="SN"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="与当事人关系"	type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="保证金额"		type="M"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保日期"		type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保物名称"		type="C100"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保范围"		type="C200"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保期限"		type="SN"	minOccurs="0" maxOccurs="1"/>
