		<CASE:element name="裁定内容"		type="C200"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="担保文书序号"	type="BB"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="更新时间" 		type="DT" 	minOccurs="1" maxOccurs="1"/>   
    </CASE:sequence>
  </CASE:complexType>
  <CASE:complexType name="执行担保情况">
    <CASE:sequence>
		<CASE:element name="执行担保情况" type="执行担保情况1"  minOccurs="0" maxOccurs="32768"/>
	</CASE:sequence>
  </CASE:complexType>
  <CASE:element name="实施类案件执行担保情况" type="执行担保情况"/>
</CASE:schema>