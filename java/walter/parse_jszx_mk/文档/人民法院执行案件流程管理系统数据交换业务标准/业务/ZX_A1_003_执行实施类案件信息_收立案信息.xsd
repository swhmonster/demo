		</CASE:sequence>
	</CASE:complexType>
	<CASE:complexType name="收案和立案信息">
		<CASE:sequence>
			<CASE:element name="收案和立案信息"	type="收案和立案信息1"	minOccurs="0" maxOccurs="1"/>
		</CASE:sequence>
	</CASE:complexType>
	<CASE:element name="实施类案件收案和立案信息" type="收案和立案信息"/>
