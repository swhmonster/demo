<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

	<CASE:include schemaLocation="_0A_类型_基础.xsd" />
	<CASE:complexType name="约谈1">
		<CASE:sequence>
			<CASE:element name="案件标识"		type="ID"	minOccurs="1" maxOccurs="1"/>
			<CASE:element name="序号_约谈"		type="SN"	minOccurs="1" maxOccurs="1"/>
			<CASE:element name="被约谈人"		type="BB"	minOccurs="1" maxOccurs="1"/>
			<CASE:element name="约谈时间"		type="DT"	minOccurs="1" maxOccurs="1"/>
			<CASE:element name="约谈方式"		type="C50"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="提供线索期限"	type="TN"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="约谈事由"		type="C100"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="约谈场所"		type="C200"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="约谈参加人员"	type="BB"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="是否同意终本"	type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="更新时间" 		type="DT" 	minOccurs="1" maxOccurs="1"/>   
		</CASE:sequence>
	</CASE:complexType>
	<CASE:complexType name="约谈">
		<CASE:sequence>
			<CASE:element name="约谈" type="约谈1"  minOccurs="0" maxOccurs="32768"/>
		</CASE:sequence>
	</CASE:complexType>
	<CASE:element name="实施类案件约谈" type="约谈"/>
</CASE:schema>
