<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">

  <CASE:include schemaLocation="_0A_类型_基础.xsd" />
  <CASE:complexType name="结案信息1">
    <CASE:sequence>
		<CASE:element name="案件标识"		type="ID"	minOccurs="1" maxOccurs="1"/>
		<CASE:element name="结案日期"		type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="结案案由"		type="SN"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="结案方式"		type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="结案文书"		type="SN"	minOccurs="0" maxOccurs="1"/> 
		<CASE:element name="结案文书送达日期"		type="D"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="结案文书送达方式"		type="SDM"	minOccurs="0" maxOccurs="1"/>
		<CASE:element name="更新时间" 		type="DT" 	minOccurs="1" maxOccurs="1"/>   
    </CASE:sequence>
  </CASE:complexType>
  <CASE:complexType name="结案信息">
    <CASE:sequence>
		<CASE:element name="结案信息" type="结案信息1"  minOccurs="0" maxOccurs="1"/>
	</CASE:sequence>
  </CASE:complexType>
  <CASE:element name="审查类案件结案信息" type="结案信息"/>
</CASE:schema>
