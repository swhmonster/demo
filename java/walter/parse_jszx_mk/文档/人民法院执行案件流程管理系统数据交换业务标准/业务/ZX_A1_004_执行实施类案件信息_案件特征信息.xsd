<?xml version="1.0" encoding="UTF-8"?>
<CASE:schema   xmlns="http://dataexchange.court.gov.cn/2015/data"
          xmlns:CASE="http://www.w3.org/2001/XMLSchema"
     targetNamespace="http://dataexchange.court.gov.cn/2015/data"
  elementFormDefault="qualified"
attributeFormDefault="unqualified">
 
	<CASE:include schemaLocation="_0A_类型_基础.xsd" />
	
	<CASE:complexType name="案件特征信息1">
		<CASE:sequence>
			<CASE:element name="案件标识"		type="ID"	minOccurs="1" maxOccurs="1"/>	
			<CASE:element name="未成年人"		type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="地域涉及"		type="SDM"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="案件涉及"		type="MDM"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="大案"			type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="要案"			type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="新类型案件"		type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="疑难案件"		type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="集团案件"		type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="矛盾激化案件"	type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="本院重点案件"	type="B"	minOccurs="0" maxOccurs="1"/>
			<CASE:element name="更新时间" 	    type="DT" 	minOccurs="1" maxOccurs="1"/>
		</CASE:sequence>
	</CASE:complexType>
	<CASE:complexType name="案件特征信息">
		<CASE:sequence>
			<CASE:element name="案件特征信息" type="案件特征信息1"  minOccurs="0" maxOccurs="1"/>
		</CASE:sequence>
	</CASE:complexType>
	<CASE:element name="实施类案件案件特征信息" type="案件特征信息"/>
</CASE:schema>
